<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Docutain_SDK_Example_.NET_MAUI.TextResultPage"
             Title="TextResultPage">
    <ContentPage.Content>
        <Grid>
            <ScrollView Padding="16">
                <Label x:Name="resultLabel"/>
            </ScrollView>
            <ActivityIndicator x:Name="activityIndicator" IsRunning="True" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand"/>
        </Grid>
    </ContentPage.Content>
</ContentPage>